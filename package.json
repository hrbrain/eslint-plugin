{
  "name": "@hrbrain/eslint-plugin",
  "version": "0.3.1",
  "repository": "https://github.com/hrbrain/eslint-plugin.git",
  "license": "Apache-2.0",
  "author": "hrb-yamamoto-katsuya <yamamoto.katsuya@hrbrain.co.jp>",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "gen:rule": "hygen generate rule",
    "gen:sandbox": "hygen generate sandbox",
    "lint-staged": "lint-staged",
    "release": "shipjs prepare",
    "sandbox": "eslint --fix ./sandbox/**/*  --rulesdir dist/rules",
    "test": "mocha --require ts-node/register tests/lib/**/*.spec.ts",
    "typecheck": "tsc --project ./tsconfig.build.json --noEmit",
    "update": "ts-node tools/update.ts"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^3.6.1",
    "@typescript-eslint/parser": "^3.10.1",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/eslint": "^7.2.0",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^8.0.0",
    "@types/prettier": "^2.1.0",
    "@types/react": "^16.9.43",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "chai": "^4.2.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "hygen": "^6.0.4",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.3",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "shipjs": "0.21.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "eslint": "^6.0.0 || >=7.0.0"
  },
  "engines": {
    "node": "10.x || 12.x || 14.x",
    "yarn": ">=1.22.4"
  }
}
